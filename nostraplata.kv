#: import ListItemButton kivy.uix.listview.ListItemButton
#: import ListAdapter kivy.adapters.listadapter.ListAdapter


MainScreen:

<MainScreen@BoxLayout>:
    orientation: 'vertical'

    ActionBar:
        pos_hint: {'top':1}
        ActionView:
            use_separator: True
            ActionPrevious:
                title: 'Nostra Plata'
                with_previous: False
            ActionOverflow:
            ActionButton:
                text: 'Btn1'

    ListView:
        adapter:
            ListAdapter(data=[str(index) for index in range(60)], cls=ListItemButton)
